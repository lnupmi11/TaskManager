
<h1 class="mt-4">Users</h1>

@await Html.PartialAsync("_StatusMessage", (string)ViewBag.InfoMessage)

<div id="usersPartial"></div>

<script>
    $('#usersPartial').load('@Url.Content("~/UserManagement/UsersPartial")');

    function updateData() {
            $.ajax({
                type: "GET",
                url: '@Url.Content("~/UserManagement/UsersPartial")',
                success: function (result) {
                    $('#usersPartial').html(result);
                }
            });
    }

</script>

@section Scripts {
    @await Html.PartialAsync("_ValidationScriptsPartial")
}